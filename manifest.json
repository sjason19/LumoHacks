{
  "manifest_version": 2,
  "name": "Senti",
  "description": "This extension checks up on how you're feeling and gives you the appropriate resources",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "clarifai.js",
      "background.js"
    ]
  },
  "options_page": "options.html"
}